<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itheima.dao.ManagerDao" >
    <!-- 查询管理员 -->
    <select id="findManager" parameterType="Integer" resultType="Manager">
	    select *
	    from t_manager
	    where mid = #{mid}   
    </select>
</mapper>